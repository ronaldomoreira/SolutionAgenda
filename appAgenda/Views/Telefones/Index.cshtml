@model IEnumerable<appAgenda.ViewModels.Agenda.TelefoneViewModel>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TipoTel)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DDI)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DDD)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NumeroTelefone)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TipoTel)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DDI)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DDD)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NumeroTelefone)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.IdTelefone }) |
            @Html.ActionLink("Details", "Details", new { id=item.IdTelefone }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.IdTelefone })
        </td>
    </tr>
}

</table>
